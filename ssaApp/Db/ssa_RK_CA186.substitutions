# Float outputs converted to integers, modbus function code 6.
file "ao_slope.template" {
pattern
{            R,                      DESC,          PORT, OFFSET,    EOFF,   ESLO, DRVL, DRVH, LOPR, HOPR, PREC,     EGU}
