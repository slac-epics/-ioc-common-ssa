# asyn record for the underlying asyn octet port
file "asynRecord.db" { 
pattern
{        R, PORT, ADDR, IMAX, OMAX}
{OctetAsyn, $(P),    0,  125,  125}
}
#--------------------------------------------------------------------------------------------------

# Binary outputs, modbus function code 6.
file "bo_word.template" {
pattern
{            R,                      DESC,          PORT, OFFSET,   MASK,      ZNAM,     ONAM}
{     DCEnable,       "DC Enable/Disable", RF00_Out_Word,      0, 0x0001, "Disable", "Enable"}
{     RFEnable,       "RF Enable/Disable", RF00_Out_Word,      1, 0x0001, "Disable", "Enable"}
{FaultIntReset,    "Internal Fault Reset", RF00_Out_Word,      3, 0x0001,        "",  "Reset"}
{FaultExtReset,    "External Fault Reset", RF00_Out_Word,      3, 0x0002,        "",  "Reset"}
{ WarningReset,           "Warning Reset", RF00_Out_Word,      3, 0x0004,        "",  "Reset"}
{     XPReboot,           "X-Port Reboot", RF00_Out_Word,      4, 0x0001,        "", "Reboot"}
{    SysReboot,           "System Reboot", RF00_Out_Word,      5, 0x0001,        "", "Reboot"}
{    PulseMode,              "Pulse Mode", RF00_Out_Word,      6, 0x0001, "Disable", "Enable"}
{      APD_ATT,            "APD/ATT Mode", RF00_Out_Word,      7, 0x0001,     "APD",    "ATT"}
}

