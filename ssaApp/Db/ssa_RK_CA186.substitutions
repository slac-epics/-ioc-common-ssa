file "ai_pwrFwd_autosave.template" {
pattern
{           R,                       DESC,         PORT, OFFSET,   PREC,   LOLO,    LOW,   HIGH,   HIHI,     LLSV,      LSV,      HSV,     HHSV,     EGU,       SCAN}
