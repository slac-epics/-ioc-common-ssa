# ao record template for register outputs
record(ao, "$(P)$(R)") {
    field(DTYP,"asynInt32")
    field(OUT,"@asynMask($(PORT) $(OFFSET) $(BITS))MODBUS_DATA")
    field(LINR,"LINEAR")
    field(EGUL,"$(EGUL)")
    field(EGUF,"$(EGUF)")
    field(DRVL,"$(DRVL)")
    field(DRVH,"$(DRVH)")
    field(LOPR,"$(LOPR)")
    field(HOPR,"$(HOPR)")
    field(EGU, "$(EGU)")
    field(PREC,"$(PREC)")
}

