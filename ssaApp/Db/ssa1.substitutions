# These are the B0 outputs done with bit access.
file "bo_word.template"
{ pattern
{P,           R,        PORT,      OFFSET,   MASK,	ZNAM,  ONAM}
{SSA1:,    OUT1,        RF1_Out_Word,    0,     0x0001,	0, 	1}
{SSA1:,    OUT2,        RF1_Out_Word,    1,     0x0001,	0,	1}
{SSA1:,    OUT4,        RF1_Out_Word,    3,     0x0001,	0,	1}
{SSA1:,    OUT5,        RF1_Out_Word,    4,     0x0001,	0,	1}
{SSA1:,	   OUT6,	RF1_Out_Word,    5,     0x0001,	0,	1}
}

# These are the A0 ouput donw with word write
file "longout.template"
{ pattern
{P,           R,        PORT,      OFFSET}
{SSA1:,    OUT3,        RF1_Out_Word,    2}
}

# There are the Inputs done with bit acess
file "bi_word.template"
{ pattern
{P,           R,        PORT,      OFFSET,   MASK,	ZNAM,  ONAM, 	ZSV, 	OSV, 	 SCAN}
{SSA1:,    BIN1,        RF1_Out_Word,    0,     0x0001,	0, 	1,	NO_ALARM, MAJOR, "I/O Intr"}

}

