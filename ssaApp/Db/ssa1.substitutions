{SSA1:,   IN45,     RF1_In_Word,    44,	0xFFFF,  0,    -0.1, 1e3,  0,  1,   "I/O Intr"}
{SSA1:,   IN46,     RF1_In_Word,    45,	0xFFFF,  0,    -0.1, 1e3,  0,  1,   "I/O Intr"}
{SSA1:,   IN48,     RF1_In_Word,    47,	0xFFFF,  0,    -0.1, 1e3,  0,  1,   "I/O Intr"}
